{
	"info": {
		"name": "Auth API",
		"description": "Postman collection for Auth API routes",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Login",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"username\": \"\",\n  \"password\": \"\"\n}"
				},
				"url": {
					"raw": "{{baseUrl}}/login",
					"host": ["{{baseUrl}}"],
					"path": ["login"]
				}
			},
			"response": []
		},
		{
			"name": "Refresh Token",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"refreshToken\": \"\"\n}"
				},
				"url": {
					"raw": "{{baseUrl}}/refresh",
					"host": ["{{baseUrl}}"],
					"path": ["refresh"]
				}
			},
			"response": []
		},
		{
			"name": "Logout",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Authorization",
						"value": "Bearer {{token}}"
					}
				],
				"url": {
					"raw": "{{baseUrl}}/logout",
					"host": ["{{baseUrl}}"],
					"path": ["logout"]
				}
			},
			"response": []
		},
		{
			"name": "Get User",
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Bearer {{token}}"
					}
				],
				"url": {
					"raw": "{{baseUrl}}/user",
					"host": ["{{baseUrl}}"],
					"path": ["user"]
				}
			},
			"response": []
		}
	]
}
